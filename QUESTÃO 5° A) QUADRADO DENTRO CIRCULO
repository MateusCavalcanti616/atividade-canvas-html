<script>
  let canvas = document.querySelector('canvas');
  let ctx = canvas.getContext('2d');
 

  
    ctx.beginPath( );
    ctx.arc (300,300, 90, 0, 2 * Math.PI);
    ctx.stroke();
    ctx.fillStyle="darkblue";
    ctx.fillRect(260,260,80,80);


  
    

</script>
